<script type="application/javascript">
    
    notificationCountPolling('', '/World-Finals-2017');

    
    $(function() {
        $("#feedbackLink").click(function() {
            $.get("/World-Finals-2017/feedback-form;jsessionid=EjfAuYeOjAMyMPDxXg4Se2AcdhIjtNso4UJHAFlJ.2014myicpc06", function(data) {
                $("#feedbackWrapper").html(data);
                $("#feedbackModal").modal('show');
            });
        });
    });

    $("#notification-counter-link").click(function() {
        var $featuredNotificationContainer = $("#featured-notification-container");
        if (!$featuredNotificationContainer.is(":visible")) {
            $.get("/World-Finals-2017/notification/featured-panel;jsessionid=EjfAuYeOjAMyMPDxXg4Se2AcdhIjtNso4UJHAFlJ.2014myicpc06", function(data) {
                $featuredNotificationContainer.html(data);
                $featuredNotificationContainer.slideDown();
                $(window).scrollTop(0);
            });
        } else {
            $featuredNotificationContainer.slideUp();
        }
    });

    
    showShareDialog = function(notificationId) {
        $.get('/World-Finals-2017/notification/;jsessionid=EjfAuYeOjAMyMPDxXg4Se2AcdhIjtNso4UJHAFlJ.2014myicpc06' +notificationId + '/share', function(data) {
            $('#shareNotificationDialogContainer').html(data);
            $('#shareNotificationDialog').modal('show');
        })
    }

    function profilePictureError(image) {
        image.onerror = "";
        image.src = "/images/default-profile-small.png;jsessionid=EjfAuYeOjAMyMPDxXg4Se2AcdhIjtNso4UJHAFlJ.2014myicpc06";
        return true;
    }
    profilePictureError

    if (typeof localStorage !== "undefined") {
        localStorage["currentContest"] = 'World-Finals-2017';
    }

    
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-44308219-1', 'auto');
    ga('send', 'pageview');

</script>
